Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

dzen2.font: PragmataPro for Powerline:size=7

URxvt*font: xft:PragmataPro for Powerline:size=8
URxvt*boldFont: xft:PragmataPro for Powerline:size=8
urxvt*scrollBar: false
urxvt*boldMode: false
rxvt.scrollBar_floating: true
urxvt.scrollBar_right: true

*background: #202020
*foreground: #f8f8f8
*cursorColor: #a0a0a0

!black
*color0: #404040
*color8: #606060

!red
*color1:   #aabb99
*color9: #889977


!green
*color2:   #C66F6F
*color10: #A44D4D 

!yellow
*color3:   #ddccaa
*color11: #bbaa88

!blue
*color4:   #7C98C2
*color12: #667D9F

!magenta
*color5: #bb9988
*color13: #997766

!cyan
*color6:   #F4F0ED
*color14: #B5ACA2

!white
*color7:    #f8f8f8
*color15: #d6d6d6